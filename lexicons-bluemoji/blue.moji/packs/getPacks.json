{
	"lexicon": 1,
	"id": "blue.moji.packs.getPacks",
	"defs": {
		"main": {
			"type": "query",
			"description": "Get views for a list of Bluemoji packs.",
			"parameters": {
				"type": "params",
				"required": ["uris"],
				"properties": {
					"uris": {
						"type": "array",
						"items": { "type": "string", "format": "at-uri" },
						"maxLength": 25
					}
				}
			},
			"output": {
				"encoding": "application/json",
				"schema": {
					"type": "object",
					"required": ["packs"],
					"properties": {
						"packs": {
							"type": "array",
							"items": {
								"type": "ref",
								"ref": "blue.moji.packs.defs#packViewBasic"
							}
						}
					}
				}
			}
		}
	}
}
